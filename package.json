{
  "name": "@bryce-loskie/utils",
  "version": "0.1.38",
  "packageManager": "pnpm@7.9.5",
  "description": "Collection of typescript utils by @guygubaby",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/guygubaby",
  "homepage": "https://github.com/guygubaby/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/utils.git"
  },
  "bugs": {
    "url": "https://github.com/guygubaby/utils/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./dom": {
      "types": "./dist/dom/index.d.ts",
      "require": "./dist/dom/index.js",
      "import": "./dist/dom/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "start": "esno src/index.ts",
    "build": "tsup",
    "release": "bumpp --commit --push --tag",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "pnpm lint -- --fix",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "doc": "pnpm build && typedoc src/index.ts src/dom/index.ts",
    "typecheck": "tsc --noEmit --skipLibCheck ./src/**/*.ts"
  },
  "dependencies": {
    "@types/throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@bryce-loskie/eslint-config": "^2.0.8",
    "@types/node": "^18.11.9",
    "bumpp": "^8.2.1",
    "c8": "^7.12.0",
    "eslint": "^8.27.0",
    "esno": "^0.16.3",
    "nanoid": "^4.0.0",
    "throttle-debounce": "^5.0.0",
    "tsup": "^6.4.0",
    "typedoc": "^0.23.21",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.1"
  }
}
