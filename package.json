{
  "name": "@bryce-loskie/utils",
  "version": "0.1.20",
  "description": "Collection of ts utils by @guygubaby",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./dom": {
      "require": "./dist/dom.js",
      "import": "./dist/dom.mjs",
      "types": "./dist/dom.d.ts"
    }
  },
  "funding": "https://github.com/sponsors/guygubaby",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/guygubaby/utils/issues"
  },
  "homepage": "https://github.com/guygubaby/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/utils.git"
  },
  "keywords": [],
  "files": [
    "dist",
    "dom.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "start": "esno src/index.ts",
    "build": "tsup src/index.ts src/dom.ts --format cjs,esm --dts --no-splitting",
    "release": "bumpp --commit --push --tag && npm publish",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "pnpm lint -- --fix",
    "test": "jest",
    "doc": "typedoc src/index.ts src/dom.ts"
  },
  "devDependencies": {
    "@bryce-loskie/eslint-config": "^1.0.6",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.7",
    "bumpp": "^7.1.1",
    "eslint": "^8.6.0",
    "eslint-plugin-jest": "^25.3.4",
    "esno": "^0.13.0",
    "jest": "^27.4.5",
    "throttle-debounce": "^3.0.1",
    "ts-jest": "^27.1.2",
    "tsup": "^5.11.10",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.4",
    "vite": "^2.7.10"
  },
  "dependencies": {
    "@types/throttle-debounce": "^2.1.0"
  }
}
